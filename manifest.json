{
  "manifest_version": 3,
  "name": "Vlinq Sitemap URL Opener",
  "version": "1.0",
  "description": "Fetch sitemap from store.vlinq.co and open URLs in new tabs.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://store.vlinq.co/*"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }